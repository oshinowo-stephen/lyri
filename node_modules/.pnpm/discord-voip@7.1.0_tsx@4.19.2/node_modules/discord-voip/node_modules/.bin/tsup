#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/stone/lyri/node_modules/.pnpm/tsup@8.3.6_tsx@4.19.2_typescript@5.7.3/node_modules/tsup/dist/node_modules:/home/stone/lyri/node_modules/.pnpm/tsup@8.3.6_tsx@4.19.2_typescript@5.7.3/node_modules/tsup/node_modules:/home/stone/lyri/node_modules/.pnpm/tsup@8.3.6_tsx@4.19.2_typescript@5.7.3/node_modules:/home/stone/lyri/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/stone/lyri/node_modules/.pnpm/tsup@8.3.6_tsx@4.19.2_typescript@5.7.3/node_modules/tsup/dist/node_modules:/home/stone/lyri/node_modules/.pnpm/tsup@8.3.6_tsx@4.19.2_typescript@5.7.3/node_modules/tsup/node_modules:/home/stone/lyri/node_modules/.pnpm/tsup@8.3.6_tsx@4.19.2_typescript@5.7.3/node_modules:/home/stone/lyri/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../tsup@8.3.6_tsx@4.19.2_typescript@5.7.3/node_modules/tsup/dist/cli-default.js" "$@"
else
  exec node  "$basedir/../../../../../tsup@8.3.6_tsx@4.19.2_typescript@5.7.3/node_modules/tsup/dist/cli-default.js" "$@"
fi
