#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/stone/lyri/node_modules/.pnpm/discord-player-youtubei@1.4.2/node_modules/discord-player-youtubei/bin/node_modules:/home/stone/lyri/node_modules/.pnpm/discord-player-youtubei@1.4.2/node_modules/discord-player-youtubei/node_modules:/home/stone/lyri/node_modules/.pnpm/discord-player-youtubei@1.4.2/node_modules:/home/stone/lyri/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/stone/lyri/node_modules/.pnpm/discord-player-youtubei@1.4.2/node_modules/discord-player-youtubei/bin/node_modules:/home/stone/lyri/node_modules/.pnpm/discord-player-youtubei@1.4.2/node_modules/discord-player-youtubei/node_modules:/home/stone/lyri/node_modules/.pnpm/discord-player-youtubei@1.4.2/node_modules:/home/stone/lyri/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../discord-player-youtubei/bin/index.js" "$@"
else
  exec node  "$basedir/../discord-player-youtubei/bin/index.js" "$@"
fi
